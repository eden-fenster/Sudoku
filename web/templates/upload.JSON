{
  "#comment": "",
  "html": {
    "@lang": "en",
    "head": {
      "title": "Sudoku Solver Server"
    },
    "body": {
      "form": {
        "@action": "http://localhost:5000/results",
        "@method": "POST",
        "@enctype": "multipart/form-data",
        "p": "Welcome to the Sudoku Solver server, upload a file.",
        "input": [
          {
            "@type": "file",
            "@name": "file",
            "@accept": ".txt"
          },
          {
            "@type": "submit",
            "@value": "Upload"
          }
        ]
      }
    }
  }
}